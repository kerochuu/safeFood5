<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

  <settings>
  	<setting name="jdbcTypeForNull" value="NULL"/>
  </settings>
  
  
	<typeAliases>
		<typeAlias type="com.ssafy.spring.vo.Food" alias="Food"/> 
		<typeAlias type="com.ssafy.spring.vo.User" alias="User"/> 
		<typeAlias type="com.ssafy.spring.vo.Qna" alias="Qna"/> 
	</typeAliases>

	<mappers>
	
		<mapper resource="mapper/Food.xml"/>
		<mapper resource="mapper/User.xml"/>
		<mapper resource="mapper/Qna.xml"/>
	</mappers>
</configuration>  
